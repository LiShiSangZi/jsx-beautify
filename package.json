{
  "name": "jsx-beautify",
  "displayName": "jsx-beautify",
  "description": "I am still working on it. So please do not download!!",
  "version": "0.0.2",
  "publisher": "gogocrow",
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.sayHello"
  ],
  "main": "./extension",
  "contributes": {
    "snippets": [{
      "language": "javascriptreact",
      "path": "./snippets/javascriptreact.json"
    }],
    "commands": [{
      "command": "extension.sayHello",
      "title": "Hello World"
    }]
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "eslint": "^3.6.0",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  },
  "dependencies": {
    "prettydiff": "^99.0.1"
  }
}
