{
  "name": "jsx-beautify",
  "displayName": "jsx-beautify",
  "description": "I am still working on it. So please do not download!!",
  "version": "0.0.3",
  "publisher": "gogocrow",
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./extension",
  "contributes": {
    "snippets": [{
      "language": "javascriptreact",
      "path": "./snippets/javascriptreact.json"
    }],
    "commands": [{
      "command": "gogocrow.beautify",
      "title": "Beautify File"
    }],
    "configuration": {
      "title": "jsx-beautify configuration",
      "properties": {
        "gogocrow.beautify.jsbeautifyrc": {
          "description": "Specifies the path where the jsbeautifyrc file.",
          "type": "string",
          "default": "" 
        }
      }
    }
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "eslint": "^3.6.0",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  },
  "dependencies": {
    "prettydiff": "^99.0.1"
  }
}
